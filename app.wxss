page {
    color: #444;
    box-sizing: border-box;
}

input {
    color: #444;
}

.f-c {
    display: flex;
    align-items: center;
    justify-content: center;
}

.f-c-l {
    display: flex;
    align-items: center;
    justify-content: left;
}

.f-m {
    display: flex;
}
.text-right{
    text-align:right;
}
.button {
    background: tomato;
    color: #fff;
    margin-top: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.button .icon {
    margin-right: 15rpx;
}

.price {
    color: tomato;
    font-weight: 800;
}

.air-loading {
    width: 80rpx;
    height: 80rpx;
    border: 12rpx solid #444;
    border-radius: 530rpx;
    animation: rod 0.5s infinite;
    line-height: 150rpx;
    border-left: rgba(90, 90, 90, 0.1);
    text-align: center;
    font-size: 33rpx;
}

.loadingpage {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}

.bline, .lline, .rline {
    position: relative;
}

.bline:before {
    content: '';
    display: block;
    left: -50%;
    right: -50%;
    bottom: -1px;
    position: absolute;
    border-bottom: 1px solid #eee;
    height: 1px;
    transform: scale(0.5);
}

.lline:after {
    content: '';
    width: 1px;
    top: -50%;
    left: 0;
    bottom: -50%;
    position: absolute;
    border-left: 1px solid #eee;
    transform: scale(0.5);
}

.rline:after {
    content: '';
    width: 3px;
    top: -50%;
    right: -1px;
    bottom: -50%;
    position: absolute;
    border: none !important;
    border-right: 1px solid #eee !important;
    transform: scale(0.5);
}
@keyframes rod {
    10% {
        transform: rotate(45deg);
    }

    30% {
        transform: rotate(90deg);
    }

    50% {
        transform: rotate(135deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes slideInUp {
    0% {
        transform: translateY(100%);
    }

    100% {
        transform: translateY(0);
    }
}
@font-face {
    font-family: 'ticket';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

